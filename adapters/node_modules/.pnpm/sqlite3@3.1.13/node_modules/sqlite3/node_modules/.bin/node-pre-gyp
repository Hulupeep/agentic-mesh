#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/sqlite3@3.1.13/node_modules/sqlite3/node_modules/node-pre-gyp/bin/node_modules:/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/sqlite3@3.1.13/node_modules/sqlite3/node_modules/node-pre-gyp/node_modules:/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/sqlite3@3.1.13/node_modules/sqlite3/node_modules:/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/sqlite3@3.1.13/node_modules:/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/sqlite3@3.1.13/node_modules/sqlite3/node_modules/node-pre-gyp/bin/node_modules:/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/sqlite3@3.1.13/node_modules/sqlite3/node_modules/node-pre-gyp/node_modules:/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/sqlite3@3.1.13/node_modules/sqlite3/node_modules:/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/sqlite3@3.1.13/node_modules:/home/xanacan/Dropbox/code/testfolders/qwentest/amp-repo/adapters/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-pre-gyp/bin/node-pre-gyp" "$@"
else
  exec node  "$basedir/../node-pre-gyp/bin/node-pre-gyp" "$@"
fi
